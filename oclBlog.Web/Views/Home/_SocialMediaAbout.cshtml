@model IEnumerable<ocLBlog.Entities.Models.SocialMedia>
@{
    Layout = null;
}

@foreach (var item in Model)
{
    <li>
        <a href="@item.Facebook" target="_blank">
            <img src="~/img/facebook.jpg" class="img-circle" style="max-height:40px;max-width:50px;min-height:40px;min-width:50px;" />
        </a>
        <a href="@item.Instagram" target="_blank">
            <img src="~/img/instagram.jpg" class="img-circle" style="max-height:40px;max-width:50px;min-height:40px;min-width:50px;" />
        </a>
        <a href="@item.LinkedIn" target="_blank">
            <img src="~/img/linkedin.jpg" class="img-circle" style="max-height:40px;max-width:50px;min-height:40px;min-width:50px;" />
        </a>
        <a href="@item.GitHub" target="_blank">
            <img src="~/img/github.jpg" class="img-circle" style="max-height:40px;max-width:50px;min-height:40px;min-width:50px;" />
        </a>
        <a href="mailto:@item.MailAddress" target="_blank">
            <img src="~/img/email.jpg" class="img-circle" style="max-height:40px;max-width:50px;min-height:40px;min-width:50px;" />
        </a>
        @*<a href="@item.Udemy" target="_blank">
            <img src="~/img/udemy.jpg" class="img-circle" style="max-height:50px;min-height:50px;max-width:50px;min-width:50px" />
        </a>*@
    </li>
}